<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fafafa"
    android:layout_marginBottom="10dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="107dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="107dp"
            android:layout_height="107dp"
            android:gravity="center"
            android:orientation="vertical">
            <LinearLayout
                android:gravity="center"
                android:layout_width="74dp"
                android:layout_height="74dp"
                android:background="@drawable/detailpage_profilebg">
                <ImageView
                    android:id="@+id/electronics_item_img"
                    android:layout_width="67dp"
                    android:layout_height="67dp" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/electronics_item_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="7dp"
                android:layout_marginLeft="34dp"
                android:layout_marginTop="11dp"
                android:textSize="11dp"
                android:text="텍스트1"
                android:textColor="#000000" />

            <LinearLayout
                android:id="@+id/elec_text2_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/electronics_item_title_tv"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="34dp">

                <ImageView
                    android:id="@+id/electronics_text2_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:src="@drawable/mydeal_question" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9dp"
                    android:text="[ "
                    android:textColor="#000000"/>
                <TextView
                    android:id="@+id/electronics_kind_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9dp"
                    android:text="kind"
                    android:textColor="#000000"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9dp"
                    android:text=" ]"
                    android:textColor="#000000"/>

                <TextView
                    android:id="@+id/electronics_item_product_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9dp"
                    android:layout_marginLeft="5dp"
                    android:text="텍스트2"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/elec_text3_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/elec_text2_layout"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="34dp">

                <ImageView
                    android:id="@+id/electronics_text3_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:src="@drawable/mydeal_money" />

                <TextView
                    android:id="@+id/electronics_item_price_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9dp"
                    android:text="텍스트3"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/elec_text4_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/elec_text3_layout"
                android:layout_marginBottom="23dp"
                android:layout_marginLeft="34dp">

                <ImageView
                    android:id="@+id/electronics_text4_img"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    android:src="@drawable/mydeal_time" />

                <TextView
                    android:id="@+id/electronics_item_peroid_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="9dp"
                    android:text="텍스트4"
                    android:textColor="#000000" />
            </LinearLayout>

            <TextView
                android:id="@+id/electronics_item_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="18dp"
                android:layout_marginTop="7dp"
                android:textSize="9dp"
                android:text="텍스트5"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/electronics_item_userid_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/electronics_item_time_tv"
                android:layout_marginRight="18dp"
                android:text="텍스트6"
                android:textSize="9dp"
                android:textColor="#fb9726" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="14dp"
                android:gravity="bottom">
                <LinearLayout
                    android:id="@+id/able_time_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/electronics_item_image1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5dp"
                        android:layout_toLeftOf="@+id/electronics_item_count_tv"
                        android:src="@drawable/mydeal_talkicon" />

                    <TextView
                        android:id="@+id/electronics_item_count_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="18dp"
                        android:text="text1"
                        android:textSize="9dp"
                        android:textColor="#51c7c9" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
